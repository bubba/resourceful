####################

.PHONY: all opt byte archclean clean depend

.SUFFIXES: .v .vo

.v.vo:
	$(COQC) $(COQDEBUG) $(COQFLAGS) $*

byte::
	$(MAKE) all "OPT="

opt::
	$(MAKE) all "OPT=-opt"

include .depend

depend::
	rm .depend
	$(COQDEP) $(LIBS) *.v *.ml *.mli >.depend

clean::
	rm -f *.cmo *.cmi *.cmx *.vo *~

archclean::
	rm -f *.cmx

